/*
    Complete the 'staircase' function below.
    The function accepts INTEGER n as parameter.
*/

function staircase(n) {
    let star = '#'
    let space = ' '
    
    for(let i = 1; i <= n; i++){
        console.log(space.repeat(n-i) + star.repeat(i))
    }

}

const n = 4

console.log(staircase(n))
